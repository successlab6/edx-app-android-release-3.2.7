    <string name="message_after_course_calendar_removed">Lịch khóa học của bạn đã được xóa.</string>
    <!-- Success Message after calendar for course update -->
    <string name="message_after_course_calendar_updated">Lịch khóa học của bạn đã được cập nhật.</string>
