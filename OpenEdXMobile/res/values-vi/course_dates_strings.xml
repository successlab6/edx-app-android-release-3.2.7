    <!-- region: Sync to calendar -->
    <!-- Title Sync to calendar  -->
    <string name="sync_to_calendar">Lịch</string>
    <!-- Title Sync to calendar  -->
    <string name="sync_to_calendar_message">Tự động đồng bộ tất cả các hạn chót và ngày đến hạn cho khóa học này với lịch của bạn.</string>
    <!-- Title to request calendar permission  -->
    <string name="title_request_calendar_permission">\“{platform_name}\” muốn truy cập lịch của bạn</string>
    <!-- Message to request calendar permission  -->
