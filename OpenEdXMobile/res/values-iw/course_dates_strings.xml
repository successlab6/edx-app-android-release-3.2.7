    <!-- region: Sync to calendar -->
    <!-- Title Sync to calendar  -->
    <string name="sync_to_calendar">סנכרון ללוח שנה</string>
    <!-- Title Sync to calendar  -->
    <string name="sync_to_calendar_message">סנכרן אוטומטית את כל המועדים והתאריכים של קורס זה עם היומן שלך.</string>
    <!-- Title to request calendar permission  -->
    <string name="title_request_calendar_permission">\“{platform_name}\” רוצה לגשת ליומן שלך</string>
    <!-- Message to request calendar permission  -->
    <string name="message_request_calendar_permission">{platform_name} מעוניין להשתמש ברשימת היומנים שלך כדי להירשם ליומן {platform_name} המותאם אישית שלך עבור קורס זה.</string>
    <!-- Title to add calendar for course  -->
    <string name="title_add_course_calendar">הוסף יומן \“{course_name}\”</string>
