    <string name="title_remove_course_calendar">הסר את היומן \“{course_name}\”</string>
    <!-- Message to remove calendar for course  -->
    <string name="message_remove_course_calendar">האם תרצה להסיר את לוח השנה של {platform_name} \“{course_name}\”?</string>
