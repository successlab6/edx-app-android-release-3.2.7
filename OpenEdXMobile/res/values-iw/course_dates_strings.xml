    <string name="message_add_course_calendar">האם תרצה להוסיף את לוח השנה של {platform_name} \“{course_name}\”? \n\nאתה יכול לערוך או להסיר את יומן הקורס שלך בכל עת בלוח השנה או בהגדרות.</string>
    <!-- Success Message after calendar for course added first time -->
    <string name="message_for_alert_after_course_calendar_added">\\"{course_name}\\" נוסף ללוח השנה של הטלפון שלך.</string>
    <!-- Success Message after calendar for course added -->
    <string name="message_after_course_calendar_added">יומן הקורס שלך נוסף.</string>
