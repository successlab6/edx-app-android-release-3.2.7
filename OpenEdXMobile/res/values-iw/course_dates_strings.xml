    <string name="title_calendar_out_of_date">לוח השנה של הקורס שלך כבר לא מעודכן</string>
    <!-- Message course calendar is out-of-date  -->
    <string name="message_calendar_out_of_date">תאריכי הקורסים שלך הועברו ויומן הקורס שלך אינו מעודכן עוד בלוח הזמנים החדש שלך.</string>
    <!-- Error message while creating calendar for the course events -->
    <string name="adding_calendar_error_message">שגיאה בהוספת לוח שנה, אנא נסה שוב מאוחר יותר</string>
