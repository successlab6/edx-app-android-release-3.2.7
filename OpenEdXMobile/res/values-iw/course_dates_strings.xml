    <string name="assessment_shift_dates_success_msg">תאריכי היעד שלך הועברו בהצלחה כדי לשמור אותך על המסלול.</string>
    <!-- Label to view shifted dates -->
    <string name="assessment_view_all_dates"><u>צפה בכל התאריכים</u></string>

